const generateNonce = function(length) {
    let text = "";
    const allowed = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";
    for(let i = 0; i < length; i++) {
        text += allowed.charAt(Math.floor(Math.random() * allowed.length));
    }
    return text;
}

