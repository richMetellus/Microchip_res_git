        },
        {
            name: 'QA',
            origin: 'https://aem-qa.microchip.com:8443',
            type: MchpEnvContext.QA,
            instance: MchpEnvContext.PUBLISHER,
            isDispatcher: MchpEnvContext.NO_DISPATCHER,
